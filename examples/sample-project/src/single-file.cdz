// Single file version for testing current compiler
function divide(a: int, b: int) -> Result<int, string> {
    if b == 0 {
        return Error("Division by zero")
    }
    return Ok(a / b)
}

function main() -> int {
    let result = divide(10, 2)
    
    match result {
        Ok(value) -> {
            return value
        }
        Error(message) -> {
            return -1
        }
    }
}